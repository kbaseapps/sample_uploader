{
    "ver": "0.0.1",
    "authors": [
        "slebras"
    ],
    "contact": "help@kbase.us",
    "categories": ["active", "upload"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "sample_file",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "dynamic_dropdown",
            "dynamic_dropdown_options" : {
                "data_source": "ftp_staging"
            }
        },
        {
            "id": "file_format",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "SESAR" ],
            "field_type" : "dropdown",
            "dropdown_options" : {
                "options": [
                    {
                        "value": "SESAR",
                        "display": "SESAR format"
                    },
                    {
                        "value": "ENIGMA",
                        "display": "ENIGMA format"
                    },
                    {
                        "value": "KBASE",
                        "display": "KBase format"
                    }
                ]
            }
        },
        {
            "id": "set_name",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {}
        },
        {
            "id": "header_row_index",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [],
                "validate_as": "int",
                "min_int": 1,
                "max_int": 10
            }

        },
        {
            "id": "id_field",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "default_values": [ "" ],
            "text_options": {}
        },
        {
            "id": "description",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {}
        },
        {
            "id": "sample_set_ref",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types": ["KBaseSets.SampleSet"]
            }
        },
        {
            "id": "output_format",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "",
                        "display": "No Output"
                    },{
                        "value": "csv",
                        "display": "CSV"
                    },{
                        "value": "xls",
                        "display": "XLSX"
                    }
                ]
            }
        },
        {
            "id": "taxonomy_source",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "n/a" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "n/a",
                        "display": "N/A"
                    }
                ]
            }
        },
        {
            "id": "num_otus",
            "optional" : false,
            "advanced" : true,
            "allow_multiple" : false,
            "default_values" : [ "20" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types": [],
                "validate_as": "int",
                "min_int": 1,
                "max_int": 10000
            }
        },
        {
            "id": "incl_seq",
            "optional" : false,
            "advanced" : true,
            "allow_multiple" : false,
            "default_values" : [ "0" ],
            "field_type" : "checkbox",
            "checkbox_options" : {
                "unchecked_value" : 0,
                "checked_value" : 1
            }
        },
        {
            "id": "otu_prefix",
            "optional" : false,
            "advanced" : true,
            "allow_multiple" : false,
            "default_values" : [ "OTU" ],
            "field_type" : "text",
            "text_options" : {}
        },
        {
            "id": "incl_input_in_output",
            "optional" : false,
            "advanced" : true,
            "allow_multiple" : false,
            "default_values" : [ "1" ],
            "field_type" : "checkbox",
            "checkbox_options" : {
                "unchecked_value" : 0,
                "checked_value" : 1
            }
        },
        {
            "id": "share_within_workspace",
            "optional" : false,
            "advanced" : true,
            "allow_multiple" : false,
            "default_values" : [ "1" ],
            "field_type" : "checkbox",
            "checkbox_options" : {
                "unchecked_value" : 0,
                "checked_value" : 1
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "sample_uploader",
            "method": "import_samples",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "sample_file",
                    "target_property": "sample_file"
                },{
                    "input_parameter": "file_format",
                    "target_property": "file_format"
                },{
                    "input_parameter": "set_name",
                    "target_property": "set_name"
                },{
                    "input_parameter": "id_field",
                    "target_property": "id_field"
                },{
                    "input_parameter": "header_row_index",
                    "target_property": "header_row_index"
                },{
                    "input_parameter": "description",
                    "target_property": "description"
                },{
                    "input_parameter": "sample_set_ref",
                    "target_property": "sample_set_ref",
                    "target_type_transform": "ref"
                },{
                    "input_parameter": "output_format",
                    "target_property": "output_format"
                },{
                    "input_parameter": "taxonomy_source",
                    "target_property": "taxonomy_source"
                },{
                    "input_parameter": "num_otus",
                    "target_property": "num_otus"
                },{
                    "input_parameter": "incl_seq",
                    "target_property": "incl_seq"
                },{
                    "input_parameter": "otu_prefix",
                    "target_property": "otu_prefix"
                },{
                    "input_parameter": "incl_input_in_output",
                    "target_property": "incl_input_in_output"
                },{
                    "input_parameter": "share_within_workspace",
                    "target_property": "share_within_workspace"
                }
            ],
            "output_mapping": [
              {
                "service_method_output_path": [0, "report_name"],
                "target_property": "report_name"
              },
              {
                "service_method_output_path": [0, "report_ref"],
                "target_property": "report_ref"
              },
              {
                "input_parameter": "set_name",
                "target_property": "id"
              }
            ]


        }
    },
    "job_id_output_field": "docker"
}
